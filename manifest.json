{
	"manifest_version": 3,
	"name": "Piazzanalytics",
	"short_name": "Piazzanalytics",
	"version": "1.0",
	"description": "Provides some analytics visualizations about post activity and waiting times for Piazza classes.",
	"author": "Oz Kilic",
	"icons": {
		"128": "icon.png"
	},
	"content_scripts": [
		{
			"js": [
				"script.js",
				"chart.js",
				"chart_box_violin.js"
			],
			"matches": [
				"*://piazza.com/*"
			]
		}
	],
	"web_accessible_resources": [
		{
			"resources": [
				"modal.html",
				"charts.html"
			],
			"matches": [
				"*://piazza.com/*"
			]
		}
	]
}